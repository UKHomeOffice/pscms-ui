{
  "name": "client",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "@datapunt/matomo-tracker-react": "^0.3.1",
    "@highpoint/react-loader-advanced": "^2.0.0",
    "@react-keycloak/web": "^3.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@ukhomeoffice/formio-gds-template": "^1.0.12",
    "axios": "^0.21.1",
    "dayjs": "^1.9.6",
    "formiojs": "4.9.19",
    "govuk-colours": "^1.1.0",
    "govuk-frontend": "^3.10.1",
    "html-react-parser": "^0.10.5",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "i18next-xhr-backend": "^3.2.2",
    "jwt-decode": "^2.2.0",
    "keycloak-js": "^10.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.26.0",
    "navi": "^0.14.0",
    "node-sass": "^4.14.1",
    "powerbi-client": "^2.15.1",
    "prop-types": "^15.7.2",
    "query-string": "^5.1.1",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^2.2.1",
    "react-formio": "^4.3.0",
    "react-global-configuration": "^1.4.1",
    "react-i18next": "^11.4.0",
    "react-iframe": "^1.8.0",
    "react-navi": "^0.14.3",
    "react-navi-helmet": "^0.14.3",
    "react-scripts": "^4.0.1",
    "secure-ls": "^1.2.6",
    "styled-components": "^5.1.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@testing-library/react": "^10.0.4",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^7.2.1",
    "axios-mock-adapter": "^1.18.1",
    "babel-eslint": "^10.1.0",
    "cheerio": "^1.0.0-rc.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^3.1.0",
    "jest-localstorage-mock": "^2.4.6",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.4.0",
    "sinon": "^9.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "CI=true react-scripts test"
    }
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "json",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/index.js",
      "src/serviceWorker.js",
      "src/i18n.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": 90
      }
    },
    "resetMocks": false,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": "npm run lint",
    "./src/**/*.{js,json,jsx,scss}": "npx prettier --write ./src"
  },
  "proxy": "http://localhost:8004",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test \"--coverage\" \"--watchAll=false\"",
    "test-coverage-watch": "react-scripts test \"--coverage\" \"--watchAll=true\"",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx --fix src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
